#include <iostream>
#include <math.h>
using namespace std;
int main () {
	int i, n, donate, total=0;
	string text;
	
	cin >> n;
	for (i=1; i<=n; i++) {
		cin >> text;
		if (text == "donate") {
			cin >> donate;
			total = total + donate;
		} else if (text == "report") {
			cout << total << endl;
		}
	}
	
	return 0;
}
